<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    style="@style/padding16"
    android:background="@color/black_50_opacity"
    android:id="@+id/zeroDataLayout"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/header_dialog"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Select"
        style="@style/txt_17_medium_blue"/>

    <RelativeLayout
        android:layout_width="match_parent"
        android:id="@+id/dynamicLayoutType"
        android:layout_below="@+id/header_dialog"
        android:layout_marginTop="@dimen/margin10"
        android:layout_height="wrap_content">
        <ListView
            android:id="@+id/list"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

    </RelativeLayout>



</RelativeLayout>
